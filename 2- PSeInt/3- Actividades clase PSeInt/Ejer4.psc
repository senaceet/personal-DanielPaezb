Algoritmo Area_triangulo
	// Daniel Páez
	Definir Area,Base,Altura Como Real;
	Escribir 'Digite la medida de la base del triángulo: ';
	Leer Base;
	Escribir 'Digite la medida de la altura del tríangulo: ';
	Leer Altura;
	Area <- (Base*Altura)/2;
	Escribir 'El area del triángulo es: ',Area;
FinAlgoritmo

